  <div class="container w-100 mb-3">
    <div class="row justify-content-center">
      <div class="col-sm-12 col-md-12 col-lg-6">
        <div class="container-list">
          <h1 class="big-title">Create a New List</h1>
          <div class="form-new">
            <%= simple_form_for @list do |f| %>
              <%= f.input :name %>
              <%= f.submit %>
            <% end %>
          </div>
        </div>
      </div>
    </div>
  </div>
